    "dotenv": "^8.0.0",
    "diff": ">=3.5.0",
    "graphql": "^14.4.2",
    "graphql-cli": "^3.0.12",
    "graphql-yoga": "^1.18.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.14",
    "mem": ">=4.0.0",
    "nodemailer": "^6.4.16",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "open": ">=6.0.0",
    "prisma": "^3.1.0",
    "prisma-binding": "^2.3.15",
    "stripe": "^7.4.0"
  },
  "license": "MIT",
  "repository": {
    "url": "wesbos.com"
  },
